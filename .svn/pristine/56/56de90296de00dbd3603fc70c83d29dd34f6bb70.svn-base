<!--公用尾部-->

<!--公用尾部-->

  <script id="tpl_con_box" type="text/html">
    <div class="fromBox">
      {{if !isShow}}
        
        {{if appCode}}
          <input type="hidden" value="{{appCode}}" class="appCode" />
        {{/if}}
      {{/if}}

      <div class="boxInput clear">
        <label class="f_lab">
          {{if !isShow}}
            <span class="red">*</span>
            应用渠道
          {{else}}
            应用编号
          {{/if}}
          
        </label>
        {{if isShow}}
          <span class="con_box">{{appCode}}</span>
        {{else}}
        <select data-name="appType" class="sel_box t required">
          <option value="h5game-d" {{if channle === "h5game-d"}}selected{{/if}}>冰趣</option>
          <option value="h5game" {{if channle === "h5game"}}selected{{/if}}>钱宝</option>
        </select>
        {{/if}}
      </div>

      <div class="boxInput">
        <label class="f_lab">
          {{if !isShow}}
            <span class="red">*</span>
          {{/if}}
          应用名称
        </label>

        {{if isShow}}
        <span class="con_box">{{appName}}</span>
        {{else}}
        <input class="text_con required appName" type="text" value="{{appName}}" placeholder="请输入应用名称" />
        {{/if}}
      </div>

      <div class="boxInput">
        <label class="f_lab f-left">
          {{if !isShow}}
            <span class="red">*</span>
          {{/if}}
          应用Icon
        </label>

        <div class="fileBox">
          {{if !isShow}}
            <input type="hidden" class="required imgUrl appIcon" value="{{appIcon}}"/>
          {{/if}}
          <img style="display: {{if appIcon}}inherit{{else}}none{{/if}}" class="file_btn" src="{{appIcon}}" alt=""/>
          {{if !isShow}}
          <div class="file_btn btn_file btn_file_box">
            <div class="bg"></div>
          </div>
          {{/if}}
        </div>
        {{if !isShow}}
          <div class="file_ms">分辨率为150*150px（添加角标），支持支持jpg、png、gif 格式图片</div>
        {{/if}}
      </div>

      {{if isShow}}
        {{if appKey}}
        <div class="boxInput clear">
          <label class="f_lab">
            应用 AppKey
          </label>
          <span class="con_box">{{appKey}}</span>
        </div>
        {{/if}}

        {{if loginKey}}
        <div class="boxInput clear">
          <label class="f_lab">
            登录 LoginKey
          </label>
          <span class="con_box">{{loginKey}}</span>
        </div>
        {{/if}}

        {{if payKey}}
        <div class="boxInput clear">
          <label class="f_lab">
            付款 PayKey
          </label>
          <span class="con_box">{{payKey}}</span>
        </div>
        {{/if}}

      {{/if}}

      <div class="boxInput clear">
        <label class="f_lab {{if !isShow}}s_lab{{/if}}">
          {{if !isShow}}
            <span class="red">*</span>
          {{/if}}
          应用描述
        </label>
        {{if isShow}}
          <span class="con_box">{{appDesc}}</span>
        {{else}}
          <div class="textarea">
            <textarea class="required appDesc" cols="30" rows="10">{{appDesc}}</textarea>
          </div>
        {{/if}}
      </div>

      <div class="boxInput clear">
        <label class="f_lab {{if !isShow}}s_lab{{/if}}">
          {{if !isShow}}
            <span class="red">*</span>
          {{/if}}
          游戏url
        </label>
        {{if isShow}}
          <span class="con_box">{{appUrl}}</span>
        {{else}}
        <input data-valid="http" class="text_con appUrl required" type="text" value="{{appUrl}}" placeholder="请输入游戏url，必须以http/https开头" />
        {{/if}}
      </div>

     {{if isShow}}
     <div class="boxInput clear">
        <label class="f_lab {{if !isShow}}s_lab{{/if}}">
          支付回调url
        </label>
          <span class="con_box">{{payCallBackUrl}}</span>
      </div>
      {{/if}}


      <div class="boxInput clear">
        <label class="f_lab {{if !isShow}}s_lab{{/if}}">
          {{if !isShow}}
            <span class="red">*</span>
          {{/if}}
          游戏类型
        </label>
        {{if isShow}}
          <span class="con_box">{{gameType}}</span>
        {{else}}
        <input class="text_con gameType required" type="text" value="{{gameType}}" placeholder="请输入游戏类型" />
        {{/if}}
      </div>

      <div class="boxInput">
        <label class="f_lab {{if !isShow}}s_lab{{/if}}">
          {{if !isShow}}
            <span class="red">*</span>
          {{/if}}
          开发商
        </label>
        {{if isShow}}
          <span class="con_box">{{developers}}</span>
        {{else}}
        <input class="text_con developers required" type="text" value="{{developers}}" placeholder="请输入开发商" />
        {{/if}}
      </div>

      <div class="boxInput">
        <label class="f_lab {{if !isShow}}s_lab{{/if}}">
          游戏版权标识
        </label>
        {{if isShow}}
          <span class="con_box">
            {{if copyRightFlag === 0}}
            无
            {{/if}}
            {{if copyRightFlag === 1}}
            自研
            {{/if}}
            {{if copyRightFlag === 2}}
            代理
            {{/if}}
         </span>
        {{else}}
        <select data-name="appType" class="sel_box copyRightFlag">
          <option value="">无</option>
          <option value="1" {{if copyRightFlag === 1}}selected{{/if}}>自研</option>
          <option value="2" {{if copyRightFlag === 2}}selected{{/if}}>代理</option>
        </select>
        {{/if}}
      </div>

      <div class="boxInput">
        <label class="f_lab f-left">
          软件著作权登记证书图片
        </label>
        
        <div class="fileBox">
          {{if !isShow}}
            <input type="hidden" class="imgUrl compareSoftwareUrl" />
          {{/if}}
          <img style="display: {{if compareSoftwareUrl}}inherit{{else}}none{{/if}}" class="file_btn" src="{{compareSoftwareUrl}}" alt=""/>
          {{if !isShow}}
          <div class="file_btn btn_file btn_file_box">
            <div class="bg"></div>
          </div>
          {{/if}}
        </div>
        {{if !isShow}}
        <div class="file_ms">支持支持jpg，大小350k以内</div>
        {{/if}}
      </div>

      <div class="boxInput">
        <label class="f_lab"></label>
        {{if !isShow}}
          <div class="btn_sub sub_cur">{{if appCode}}修改{{else}}提交申请{{/if}}</div>
        {{/if}}
        <a href="appList.html" class="btn_cencal">取消</a>
      </div>
      
    </div>
  </script>

<script id="requirejs"
  src="../../lib/require.js"
  data-main="../../dist/js/h5AddApp.js"
  data-url="../../lib/">
</script>

</body>
</html>
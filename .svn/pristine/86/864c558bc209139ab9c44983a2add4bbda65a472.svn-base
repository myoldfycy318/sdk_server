package com.dome.sdkserver.bo;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

public class App implements Serializable {
	/**
	 * 主键.
	 */
	private Integer id;

	/***
	 * 类型 0 = 游戏， 1 = 软件
	 */
	private String type;

	/**
	 * 分类id.
	 */
	private Integer categoryId;
	/***
	 * 排序
	 */
	private Integer rank;
	/***
	 * 是否上架（0 = 下架， 1 = 上架）
	 */
	private String publish;

	/**
	 * 应用名称.
	 */
	private String name;

	/**
	 * 二维码图片地址.
	 */
	private String qrcodeUrl;

	/**
	 * 支持平台 0 = ios 1 = android 2 = 全平台.
	 */
	private String support;

	/**
	 * 是否推荐（0=不推荐， 1=推荐）.
	 */
	private String recommended;

	/**
	 * 应用特点.
	 */
	private String specification;

	/**
	 * 应用图标URL.
	 */
	private String iconUrl;

	/**
	 * 下载量.
	 */
	private String downloadCount;

	/**
	 * 评分.
	 */
	private BigDecimal score;

	/**
	 * 专区地址.
	 */
	private String zoneUrl;

	/**
	 * 论坛地址.
	 */
	private String forumUrl;

	/**
	 * 创建时间.
	 */
	private Date createTime;

	/**
	 * 最后更新时间.
	 */
	private Date updateTime;

	/**
	 * 应用描述.
	 */
	private String description;

	/**
	 * 实际下载量
	 */
	private Integer realCount;

	/**
	 * 是否支持钱宝支付（0 = 不支持， 1 = 支持）
	 */
	private String qbPay;

	/** 富文本ID **/
	private Integer richId;

	// added by xuefeihu
	/** SDK应用编码 */
	private String sdkAppCode;

	/**
	 * 商户编码
	 */
	private String merchantCode;

	public String getMerchantCode() {
		return merchantCode;
	}

	public void setMerchantCode(String merchantCode) {
		this.merchantCode = merchantCode;
	}

	public String getSdkAppCode() {
		return sdkAppCode;
	}

	public void setSdkAppCode(String sdkAppCode) {
		this.sdkAppCode = sdkAppCode;
	}

	public Integer getRichId() {
		return this.richId;
	}

	public void setRichId(Integer richId) {
		this.richId = richId;
	}

	public String getQbPay() {
		return this.qbPay;
	}

	public void setQbPay(String qbPay) {
		this.qbPay = qbPay;
	}

	public Integer getRealCount() {
		return this.realCount;
	}

	public void setRealCount(Integer realCount) {
		this.realCount = realCount;
	}

	public Integer getRank() {
		return this.rank;
	}

	public void setRank(Integer rank) {
		this.rank = rank;
	}

	public String getPublish() {
		return this.publish;
	}

	public void setPublish(String publish) {
		this.publish = publish;
	}

	private static final long serialVersionUID = 1L;

	public String getQrcodeUrl() {
		return this.qrcodeUrl;
	}

	public void setQrcodeUrl(String qrcodeUrl) {
		this.qrcodeUrl = qrcodeUrl;
	}

	public String getSupport() {
		return this.support;
	}

	public void setSupport(String support) {
		this.support = support;
	}

	public Integer getId() {
		return this.id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getCategoryId() {
		return this.categoryId;
	}

	public void setCategoryId(Integer categoryId) {
		this.categoryId = categoryId;
	}

	public String getName() {
		return this.name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getRecommended() {
		return this.recommended;
	}

	public void setRecommended(String recommended) {
		this.recommended = recommended;
	}

	public String getSpecification() {
		return this.specification;
	}

	public void setSpecification(String specification) {
		this.specification = specification;
	}

	public String getIconUrl() {
		return this.iconUrl;
	}

	public void setIconUrl(String iconUrl) {
		this.iconUrl = iconUrl;
	}

	public String getDownloadCount() {
		return this.downloadCount;
	}

	public void setDownloadCount(String downloadCount) {
		this.downloadCount = downloadCount;
	}

	public BigDecimal getScore() {
		return this.score;
	}

	public void setScore(BigDecimal score) {
		this.score = score;
	}

	public String getZoneUrl() {
		return this.zoneUrl;
	}

	public void setZoneUrl(String zoneUrl) {
		this.zoneUrl = zoneUrl;
	}

	public String getForumUrl() {
		return this.forumUrl;
	}

	public void setForumUrl(String forumUrl) {
		this.forumUrl = forumUrl;
	}

	public Date getCreateTime() {
		return this.createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public Date getUpdateTime() {
		return this.updateTime;
	}

	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}

	public String getDescription() {
		return this.description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getType() {
		return this.type;
	}

	public void setType(String type) {
		this.type = type;
	}

}
</html>
</div>
<!--公用头部-->

<div class="contentBox">
    <div class="menuCrumbs">
        <span class="m_wz">你当前的位置：</span>
        <a href="/merchant/addservice.html" class="m_pre">管理控制台  > </a>
        <a href="#" class="m_new">添加应用</a>
    </div>
    <div class="conBox">
        <div class="fromBox">
            <div class="tit">企业信息</div>
            <div class="boxInput">
                <label class="f_lab s_lab">
                    <span class="red">*</span>
                    应用类型
                </label>
                <select data-name="appType" class="sel_box app_gameType">
                    <option value="20000000">网站应用</option>
                    <option value="30000000">移动应用</option>
                    <option value="10000000">游戏</option>
                </select>
                <span class="error red">*必填</span>
            </div>

            <!--选择游戏类型时候显示-->
            <div class="gameType hide">
                <div class="boxInput type_Code">
                    <label class="f_lab s_lab">
                        <span class="red">*</span>
                        游戏类型
                    </label>
                    <select data-name="gameType" class="sel_box app_game_type">

                    </select>
                </div>
                <div class="boxInput type_Code">
                    <label class="f_lab s_lab">
                        <span class="red">*</span>
                        游戏属性
                    </label>
                    <select data-name="gameAttr" class="sel_box app_game_sx">

                    </select>
                </div>
                <div class="boxInput type_Code">
                    <label class="f_lab s_lab">
                        <span class="red">*</span>
                        运行平台
                    </label>
                    <select data-name="runPlatform" class="sel_box app_game_pt">
                        <option value="10" class="sel_options">PC</option>
                        <option selected value="20" class="sel_options">Android</option>
                        <option value="30" class="sel_options">IOS</option>
                        <option value="40" class="sel_options">平板</option>
                    </select>
                </div>
                <div class="boxInput">
                    <label class="f_lab">
                        <span class="red">*</span>
                        一句话简介
                    </label>
                    <input data-name="gameDesc" class="text_con required" type="text" placeholder="请输入应用名称" />
                    <span class="error red">必填项</span>
                </div>
            </div>
            <!--选择游戏类型时候显示-->

            <div class="boxInput">
                <label class="f_lab">
                    <span class="red">*</span>
                    应用名称
                </label>
                <input data-name="appName" class="text_con required" type="text" placeholder="请输入应用名称" />
                <span class="error red">必填项</span>
            </div>

            <!-- 选择移动应用类型时候显示-->
            <div class="boxInput appType hide">
                <label class="f_lab s_lab">
                    <span class="red">*</span>
                    运行平台
                </label>
                <select data-name="runPlatform" class="sel_box">
                    <option value="10" class="sel_options">PC</option>
                    <option selected value="20" class="sel_options">Android</option>
                    <option value="30" class="sel_options">IOS</option>
                    <option value="40" class="sel_options">平板</option>
                </select>
            </div>
            <!-- 选择移动应用类型时候显示结束-->

            <div class="boxInput">
                <label class="f_lab f-left">
                    <span class="red">*</span>
                    应用Icon
                </label>

                <div class="fileBox">
                    <input data-name="appIcon" type="hidden" class="required imgUrl" />
                    <img style="display: none" class="file_btn" src="" alt=""/>
                    <div class="file_btn btn_file btn_file_box">
                        <div class="bg"></div>
                    </div>
                </div>
                <span class="error red">必填项</span>
                <div class="file_ms">分辨率为150*150px（添加角标），支持支持jpg、png格式图片</div>
            </div>
            <div class="boxInput">
                <label class="f_lab s_lab">
                    <span class="red">*</span>
                    应用描述
                </label>
                <div class="textarea">
                    <textarea class="required" data-name="appDesc"  cols="30" rows="10"></textarea>
                </div>
                <span class="error red">必填项</span>
            </div>
            <div class="boxInput">
                <label class="f_lab f-left">
                    <span class="red">*</span>
                    应用截图
                </label>
                <div class="fileBox">
                    <input data-name="picUrls" type="hidden" id="fileUrl" class="required imgUrl" />
                    <div class="img_box"></div>
                    <div class="btn_file_je file_btn btn_file btn_file_box">
                        <div class="bg"></div>
                    </div>
                    <span class="error red">必填项</span>
                </div>
                <div class="file_ms">分辨率为320*533px，最好为竖图，支持支持jpg、png格式图片</div>
            </div>

<!--

            <div class="boxInput">
                <label class="f_lab">
                    视频名称
                </label>
                <input class="text_con" type="text" placeholder="请填写法人代表姓名"/>
                <span class="error"></span>
            </div>
            <div class="boxInput">
                <label class="f_lab">
                    视频上传
                </label>
                <div class="fileBox">
                    <input class="text_con" type="file"  />
                    <img class="file_btn" src="../images/file_btn.png" alt=""/>
                </div>
            </div>
            <div class="boxInput">
                <label class="f_lab">
                    视频封面
                </label>
                <div class="fileBox">
                    <input class="text_con" type="file"  />
                    <img class="file_btn" src="../images/file_btn.png" alt=""/>
                </div>
            </div>
-->


            <!--用户协议-->
            <div class="boxInput">
                <label class="f_lab"></label>
                <div class="btn_sub sub_cur">提交申请</div>
                <a href="appList.html" class="btn_cencal">取消</a>
            </div>
        </div>


    </div>
</div>

<!--公用尾部-->

<!--公用尾部-->

<script type="text/html" id="tpl_games_select">
    {{each data as option}}
    <option value="{{option.typeAttrCode}}">{{option.typeAttrName}}</option>
    {{/each}}
</script>

<script type="text/html" id="tpl_games_sx">
    {{each data as option}}
    <option value="{{option.typeAttrCode}}">{{option.typeAttrName}}</option>
    {{/each}}
</script>

<script id="requirejs"
        src="../../lib/require.js"
        data-main="../dist/js/addApp.js"
        data-url="../../lib/">
</script>

</body>
</html>